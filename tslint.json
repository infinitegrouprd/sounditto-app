{
  "extends": [
    "tslint-react",
    "tslint-config-prettier",
    "tslint-config-airbnb",
    "tslint-eslint-rules",
    "tslint-config-standard"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "member-access": false,
    "jsx-boolean-value": false,
  }
}
